{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist/worker",
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ESNext"],
    "types": ["@cloudflare/workers-types"],
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": false
  },
  "include": ["src/worker/**/*"],
  "exclude": ["node_modules", "dist", "src/client", "src/server"]
}
